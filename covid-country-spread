    "# The sigmoid function (a version of the theta-function)\n",
    "xmin=-5\n",
    "xmax=5\n",
    "Nx=100\n",
    "x=np.linspace(xmin,xmax,Nx)\n",
    "\n",
    "sigx=1.0/(1.0+np.exp(-x))\n",
    "\n",
    "plt.plot(x,sigx)"
